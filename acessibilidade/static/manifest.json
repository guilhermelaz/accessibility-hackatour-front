{
  "manifest_version": 3,
  "name": "Inclusive Journey",
  "version": "1.1",
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>",
    "http://localhost:8080/interest-point/process-url"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
